{
    "name": "Facebook Unfriender - One Click",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Unfriend Facebook friends easily with just one click.",
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["facebook-unfriender-one-click.js"]
      }
    ],
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "tabs",
        "*://*.google.com/"
    ]
  }