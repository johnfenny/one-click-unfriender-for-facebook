{
    "name": "One Click Facebook Unfriender",
    "version": "1.0",
    "description": "One Click Facebook Unfriender",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistant": false
    },
    "browser_action": {
        "default_popup": "main.html"
    },
    "permissions": [
        "contextMenus"
    ]
}