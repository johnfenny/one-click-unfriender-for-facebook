{
    "name": "One Click Facebook Unfriender",
    "version": "1.0",
    "description": "One Click Facebook Unfriender",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"]
    }],
    "browser_action": {
        "default_popup": "main.html"
    },
    "permissions": [
        "contextMenus"
    ]
}